double calculate_MW_DRbar(
   const Standard_model& sm_0l,
   const Standard_model& sm,
   const @ModelName@_mass_eigenstates& model_0l,
   @ModelName@_mass_eigenstates& model)
{
   const double MW_sm = calculate_MW_pole(sm_0l, sm);
   const double MW_bsm = calculate_MW_pole(model_0l, model);
   const double mw2 = Sqr(MW_sm) - Sqr(MW_bsm) + Sqr(model.get_M@VectorW(0)@);

   if (mw2 < 0.)
      model.get_problems().flag_running_tachyon(@ModelName@_info::@VectorW@);
   else
      model.get_problems().unflag_running_tachyon(@ModelName@_info::@VectorW@);

   return AbsSqrt(mw2);
}

