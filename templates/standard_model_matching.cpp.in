double calculate_Mhh_pole(const Standard_model& sm)
{
   const double mh = sm.get_Mhh();
   const double self_energy = Re(sm.self_energy_hh(mh));
   const double M_tree = sm.get_mass_matrix_hh();
   const double M_loop = M_tree - self_energy;

   return SignedAbsSqrt(M_loop);
}

/// calculate loop tadpoles
auto calculate_tadpole_over_vevs(@ModelName@_mass_eigenstates model) -> @ModelName@_mass_eigenstates::EWSB_vector_t
{
   model.set_ewsb_loop_order(1);
   const auto tadpole_all = model.tadpole_equations_over_vevs(); // 0L + 1L
   model.set_ewsb_loop_order(0);
   const auto tadpole_0l = model.tadpole_equations_over_vevs(); // 0L
   std::remove_const<decltype(tadpole_all)>::type tadpole_loop = tadpole_all - tadpole_0l;

   return tadpole_loop;
}

